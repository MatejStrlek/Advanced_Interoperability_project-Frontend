<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hr.algebra.advanced_interoperability_projectfrontend.HelloController">
  <children>
   <GridPane layoutX="9.0" layoutY="9.0" prefHeight="571.0" prefWidth="578.0">
     <columnConstraints>
       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
     </columnConstraints>
     <rowConstraints>
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
     </rowConstraints>
      <children>
      
        <fx:include source="menu.fxml" />
        <Button alignment="CENTER" onAction="#onHelloButtonClick" prefHeight="37.0" prefWidth="95.0" text="Get all mobiles" GridPane.columnIndex="1" />
      
        <TableView fx:id="mobileTable" prefHeight="434.0" prefWidth="578.0" GridPane.columnSpan="2" GridPane.rowIndex="2" GridPane.rowSpan="2">
          <columns>
            <TableColumn fx:id="nameColumn" prefWidth="97.0" text="Mobile name" />
            <TableColumn fx:id="brandColumn" prefWidth="102.0" text="Mobile brand" />
            <TableColumn fx:id="priceColumn" prefWidth="102.0" text="Mobile price" />
            <TableColumn fx:id="descriptionColumn" prefWidth="102.0" text="Mobile desc" />
            <TableColumn fx:id="ratingColumn" prefWidth="102.0" text="Mobile rating" />
          </columns>
        </TableView>
      </children>
   </GridPane>
  </children>
</AnchorPane>
